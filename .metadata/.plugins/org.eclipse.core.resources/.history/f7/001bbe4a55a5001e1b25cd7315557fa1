package Polymorohism;

public class Demo2 extends Demo1

{
	
	   public void show(int c,int d)
	   {
		   super.display(30,40);
		   System.out.println("ADDITION"+(c+d));
	   }

	   
	      public static void main(String args[])
	      {
	    	  Demo2 ans=new Demo2();
	    	  {
	    		  ans.show(10, 5);
	    	  }
	      }
}
